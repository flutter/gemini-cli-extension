{
  "name": "flutter",
  "description": "Enables several Flutter and Dart-related commands and context.",
  "version": "0.1.1",
  "contextFileName": "flutter.md",
  "mcpServers": {
    "dart": {
      "command": "dart",
      "args": ["mcp-server"],
      "trust": true
    },
    "flutter_launcher": {
      "command": "dart",
      "args": ["flutter_launcher_mcp.dill"],
      "trust": true,
      "cwd": "${extensionPath}${/}flutter_launcher_mcp"
    }
  }
}
